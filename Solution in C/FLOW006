#include<stdio.h>

int main()
{
    int t,x,n,i,sum;
    scanf("%d",&t);
    if(t>=1&&t<=1000){
    for(i=0;i<t;i++){
        scanf("%d",&n);
        if(n>=1&&n<=1000000){
            sum=0;
            while(n!=0)
            {
                x=n%10;
                n/=10;
                sum+=x;
            }
            printf("%d\n",sum);}}
        }
        
        return 0;
}
